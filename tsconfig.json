{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "target": "ES2020",
        "lib": [
            "ES2020",
            "ES2021.WeakRef",
            "DOM"
        ],
        "types": ["vite-plugin-monkey/global"],
        "resolveJsonModule": true,
        "moduleResolution": "nodenext",
        "module": "NodeNext",
        "skipLibCheck": true,
        "strictNullChecks": true
    }
}
